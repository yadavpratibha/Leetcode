/**
 * Problem: 172. Factorial Trailing Zeroes
 * Source: https://leetcode.com/problems/factorial-trailing-zeroes/description/
 */

class Solution {
    public int trailingZeroes(int n) {
        if(n<=0){
            return 0;
        }

        int ans=0;
        while(n>=5){
            int count=n/5;
            ans+=count;
            n/=5;
        }

        return ans;
    }
}
